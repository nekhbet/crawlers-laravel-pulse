{
    "name": "nekhbet/crawlers-laravel-pulse",
    "description": "Laravel Pulse Card to show crawlers",
    "keywords": [
        "laravel",
        "pulse",
        "crawler",
        "Sorin Trimbitas",
        "nekhbet"
    ],
    "homepage": "https://github.com/nekhbet/crawlers-laravel-pulse",
    "license": "MIT",
    "authors": [
        {
            "name": "Sorin Trimbitas",
            "email": "trimbitassorin@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "jaybizzle/crawler-detect": "^1.2",
        "laravel/pulse": "^1.0@beta",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8"
    },
    "autoload": {
        "psr-4": {
            "Nekhbet\\NotFoundMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nekhbet\\xxxxxxx\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geow\\xxxxxx\\xxxxxxxxxxxServiceProvider"
            ]
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
